---
const { meta_title, meta_description } =
  Astro.props;
---

<meta charset="utf-8" />
<link
  rel="icon"
  href="/img/icons/avi.svg"
/>
<meta
  name="viewport"
  content="width=device-width, initial-scale=1.0"
/>
<title>{meta_title}</title>
<meta
  name="description"
  content={meta_description}
/>
<!-- Used by Googe -->
<meta
  itemprop="description"
  content={meta_description}
/>
